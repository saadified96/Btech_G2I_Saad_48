#include <stdio.h>
int main(){
  printf("enter the character to be searched");
    char a;
    scanf(" %c",&a);
    char vowel[5]={'a','e','i','o','u'};
    int isvowel=0;
    for(int i=0;i<6;i++){
        if(vowel[i]==a){
            isvowel=1;
            break;

        }
        else{}
    
    }
    if(isvowel){
        printf("it is a vowel");
    }else{
        printf("not a vowel");
    }
return 0;
}
